<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.EmployeeMapper">
    <insert id="insertEmployee">
        insert into employee(id,name,username,sex,id_number,phone,status,password,create_time,update_time,create_user,update_user)
        values (#{id},#{name},#{username},#{sex},#{idNumber},#{phone},#{status},#{password},#{createTime},#{updateTime},#{createUser},#{updateUser})
    </insert>
</mapper>
